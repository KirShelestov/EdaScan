{
    "expo": {
        "name": "EdaScan",
        "slug": "edascan",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/EdaScanLogo.png",
        "scheme": "edascan",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/EdaScanLogo.png",
                "backgroundColor": "#FFA500"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.pipe_bublik.EdaScan"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/EdaScanLogo.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/EdaScanLogo.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#FFA500"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "f5b016a9-1dea-4422-ba81-9d4d5f89d45f"
            }
        }
    }
}
